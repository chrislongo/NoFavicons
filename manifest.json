{
    "name": "No Favicons",
    "version": "0.4",
    "description": "Disables favicons on Chrome tabs and bookmarks.",
    "icons": { "16": "icon-16.png", "48": "icon-48.png", "128": "icon-128.png" },
    "background_page" : "background.html",
    "options_page": "options.html",
    
    "content_scripts": [
    {
        "all_frames" : true,
        "matches": ["http://*/*", "https://*/*"],
        "js": ["nofavicons.js"],
        "run_at" : "document_start"
    }]
}
